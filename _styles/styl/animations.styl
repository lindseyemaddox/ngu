
@keyframes rotate-toggle
  0%
    transform: rotate(0deg)
  100%
    transform: rotate(-180deg)

@keyframes icon-grow
  0%
    transform: scale(0)
  50%
    transform: scale(1.5)
  100%
    transform: scale(1)

@keyframes hover-width
  0%
    width: 0px
  80%
    width: 220px
  100%
    width: 200px

@keyframes link-opacity
  0%
    opacity: 100%
  80%
    opacity: 30%
  100%
    opacity: 100%
